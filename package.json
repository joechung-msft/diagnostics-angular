{
  "name": "diagnostics-angular",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:production": "ng build --configuration production",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.html\"",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint \"src/**/*.ts\" \"src/**/*.html\"",
    "lint:prettier": "prettier --check \"src/**/*.ts\" \"src/**/*.html\"",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false",
    "test:ci": "ng test --watch=false --code-coverage",
    "test:watch": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.1",
    "@angular/common": "^20.2.3",
    "@angular/compiler": "^20.2.3",
    "@angular/core": "^20.2.3",
    "@angular/forms": "^20.2.3",
    "@angular/material": "^20.2.1",
    "@angular/platform-browser": "^20.2.3",
    "@angular/router": "^20.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-vitals": "^5.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.1",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.2.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.18.0",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "^5.10.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  }
}
